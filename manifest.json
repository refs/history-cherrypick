{
  "name": "History modifier",
  "version": "1.0",
  "description": "Remove anything from your history",
  "permissions": [
    "declarativeContent",
    "history"
  ],
  "background": {
    "scripts": [
      "index.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}